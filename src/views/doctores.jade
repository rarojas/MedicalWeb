// my-template.jade
extends layoutLogged

block head
  title Lista de Doctores de Clinica

block scripts
  script(src='/js/doctores.controller.js')

block content
  #inicio(ng-controller="doctoresController as doctoresCtrl")
    .container
      .row
        .col-md-12.col-sm-12
          .div-trans.text-center
            h3 Doctores
            .col-md-12.col-sm-12
                table(class="table table-striped table-bordered table-hover ")
                  thead
                    tr
                      th  Doctores
                      th  Doctores
                      th  Doctores
                      th  Doctores
                  tbody
                    tr(ng-repeat="doctor in doctoresCtrl.doctores" style="color:black;")
                      td {{doctor.name}}
                      td {{doctor.name}}
                      td {{doctor.name}}
                      td {{doctor.name}}
