// my-template.jade
extends funcions

block content
  #inicio
    .container
      .row
        .col-md-12.col-sm-12
          .div-trans.text-center
            h3 Doctores
            .col-md-12.col-sm-12
                table(class="table table-striped table-bordered table-hover ")
                  thead
                    tr
                      th  Doctores
                      th  Especialidad
                      th  Subespecialidad
                      th  Cedula
                  tbody
                    tr(ng-repeat="doctor in vm.doctores" style="color:black;")
                      td {{doctor.nombre}}
                      td {{doctor.especialidad}}
                      td {{doctor.sub_especialidad}}
                      td {{doctor.cedula_profesional}}
