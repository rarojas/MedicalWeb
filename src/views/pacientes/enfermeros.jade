// my-template.jade
extends ../funcions

block content
  #inicio
    .container
      .row
        .col-md-12.col-sm-12
          .div-trans.text-center
            h3 Enfermeros
            .col-md-12.col-sm-12
                table(class="table table-striped table-bordered table-hover ")
                  thead
                    tr
                      th  Nombre
                      th  Especialidad
                      th  Subespecialidad
                      th  Cedula
                  tbody
                    tr(ng-repeat="enfermero in vm.enfermeros" style="color:black;")
                      td {{enfermero.nombre}}
                      td {{enfermero.especialidad}}
                      td {{enfermero.sub_especialidad}}
                      td {{enfermero.cedula_profesional}}
