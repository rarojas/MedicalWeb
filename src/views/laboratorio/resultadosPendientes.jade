extends ../funcions

block content
  #inicio
    .container
      .row
        .col-md-12.col-sm-12
          .div-trans.text-center
            h3 Analisis
            .col-md-12.col-sm-12
              .table-responsive
                table(class="table table-striped table-bordered table-hover ")
                  thead
                    tr
                      th  Paciente
                      th  impresion
                      th  descripcion
                      th  Acciones
                  tbody
                    tr(ng-repeat="resltado in vm.resultados" style="color:black;")
                      td {{resltado.paciente.nombre}}
                      td {{resltado.impresion}}
                      td {{resltado.descripcion}}
                      td
                        a.btn.btn-primary(href="#/resultadoPendiente/{{resltado.idSolicitud}}") Analisis
              
